# OneST Backend Environment Variables
# Copy this file to .env.local and configure as needed

# Node Environment
NODE_ENV=development

# API Configuration
API_PORT=3001
API_HOST=localhost

# Weather API Configuration
WEATHER_API_TIMEOUT=10000
WEATHER_CACHE_DURATION=300000

# Logging
LOG_LEVEL=info

# CORS Configuration
CORS_ORIGIN=http://localhost:3000